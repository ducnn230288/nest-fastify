  <body class='bg-white max-xl:overflow-y-auto'>

    {{> body}}

    <script type='module' src='/src/main.ts'></script>

    <!-- jQuery 1.7.2+ or Zepto.js 1.0+ -->
    <script src='https://code.jquery.com/jquery-3.7.1.min.js'></script>

    <!--  vanilla-lazyload 17 -->
    <script src='https://cdn.jsdelivr.net/npm/vanilla-lazyload@17.8.8/dist/lazyload.min.js'></script>
    <script>
      setTimeout(() => { new LazyLoad({ callback_error: (el) => (el.src = "https://via.placeholder.com/1x1/?text="), });
      }, 1000);
    </script>

    <!-- Magnific Popup core JS file -->
    <script src='src/magnific-popup.js'></script>
    <script>
      $(document).ready(function () { $(".image-link").magnificPopup({ type: "image" }); });
    </script>

    <!-- Swiper JS -->
    <script src='https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js'></script>

    <!-- Initialize Swiper -->
    <script>
      var swiper = new Swiper(".swiper-section-experience", { hashNavigation: true, spaceBetween: 30, speed: 800,
      parallax: !0, pagination: { el: ".swiper-pag", type: "fraction", clickable: !0, }, navigation: { nextEl:
      ".swiper-button-next", prevEl: ".swiper-button-prev", }, slidesPerView: 1, breakpoints: { 1366: { slidesPerView:
      3, }, 1024: { slidesPerView: 3, }, 768: { slidesPerView: 1, }, 500: { slidesPerView: 1, }, }, }); new
      Swiper(".swiper-section-testimonials", { hashNavigation: true, spaceBetween: 30, speed: 800, parallax: !0,
      pagination: { el: ".swiper-pagination", type: "fraction", clickable: !0, }, navigation: { nextEl:
      ".swiper-button-next", prevEl: ".swiper-button-prev", }, slidesPerView: 1, });
    </script>
    <!-- Initialize Swiper end -->

