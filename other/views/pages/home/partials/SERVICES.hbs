<section class="pt-32 pb-16 font-epilogue">
      <div class="px-3 sm:px-9 md:px-20">
        <div class="mb-20 lg:flex lg:justify-between">
          <div class="px-1 sm:px-0">
            <h6
              class="px-7 py-2 border rounded-full border-gray-900 w-48 h-9 text-xs text-gray-900 flex items-center justify-center mb-4 cursor-pointer wow fadeIn" data-wow-duration="1200ms">
              {{#with (lookup paraServices 0)}}
                  {{this.code}}
              {{/with}}</h6>
            <h2 class="mb-5 lg:mb-0 text-3xl md:text-4xl lg:text-5xl text-gray-900 font-semibold wow fadeInUp" data-wow-duration="1200ms" data-wow-delay="400ms">{{#with (lookup paraServices 0)}}
                  {{this.description}}
              {{/with}}</h2>
          </div>
          <div class="px-1 lg:w-80 xl:w-96">
            <p class="text-base text-gray-500 font-light xl:mt-10 wow fadeInUp" data-wow-duration="1200ms" data-wow-delay="400ms">{{#with (lookup paraServices 1)}}
                  {{this.description}}
              {{/with}}</p>
          </div>
        </div>
        <!-- handlebars -->
        <script id="template-ourservices" type="text/x-handlebars-template">
          <ul class="px-1 lg:flex lg:justify-between lg:gap-7">
            {{#each services}}
            <li class="px-7 py-12 mb-14 md:p-14 border border-gray-300 rounded-md wow fadeInLeft" data-wow-duration="1200ms" data-wow-delay="400ms">
              <div class="mb-10">
                <img src="{{icon}}" alt="" class="w-12 h-12">
              </div>
              <span class="mb-2.5 text-sm text-gray-900 inline-block">{{#with (lookup translations 0)}}
                    {{this.name}}
                  {{/with}}</span>
              <h6 class="mb-4 text-base font-medium lg:text-xl text-gray-900">{{#with (lookup translations 0)}}
                    {{this.description}}
                  {{/with}}</h6>
              <p class="text-sm text-gray-500 font-light">{{#with (lookup translations 0)}}
                    {{this.content}}
                  {{/with}}</p>
              <div class="w-full">
                <a href="#"
                  class="hover:bg-white py-4 pl-8 mt-8 border border-gray-300 rounded-full w-64 h-12 text-xs text-gray-900 flex items-center cursor-pointer relative sm:w-full">
                  READ MORE
                  <span
                    class="absolute z-10 w-12 h-12 rounded-full border border-gray-300 flex items-center justify-center right-0">
                    <svg width="18px" height="18px" viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg">
                      <path fill="#000000"
                        d="M768 256H353.6a32 32 0 1 1 0-64H800a32 32 0 0 1 32 32v448a32 32 0 0 1-64 0V256z"></path>
                      <path fill="#000000"
                        d="M777.344 201.344a32 32 0 0 1 45.312 45.312l-544 544a32 32 0 0 1-45.312-45.312l544-544z">
                      </path>
                    </svg>
                  </span>
                </a>
              </div>
            </li>
            {{/each}}
          </ul>
        </script>
        <div class="ourservices">
        </div>
      </div>
    </section>